
<div class="wrap">
	<div class="title_line">
		<h1>Owl Carousel
			<a href="{{ panel_url('oCoder::editSlide') }}" class="page-title-action">Add New</a>
		</h1>
		<table class="wp-list-table widefat fixed striped">
		<thead>
			<tr>
				<td id="cb" class="manage-column column-cb check-column"></td>
				<th>id</th>
				<th>Name</th>
				<th>Content</th>
				<th>Image Url</th>
			</tr>
		</thead>
		<tbody id="the-list">
			{% for item in ocoder %}

				<tr> 
					<td></td>
					<td>{{item.id}}</td>
					<td><a href="{{ panel_url('oCoder::editSlide', {'id': item.id }) }}"> {{ item.name }}</a></td>
					<td> {{ item.content }}</td>
					<td> {{ item.image_link }}</td>
				</tr>
			{% else %}
			<tr> 
				<td class="colspanchange" colspan="4">Nothing.</td>
			</tr>
			
			{% endfor %}	
		</tbody>
		
			
		
		</table>
	</div>
</div>